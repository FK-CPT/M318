<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="_05Navigazione.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:_05Navigazione"
    Shell.FlyoutBehavior="Disabled"
    Title="_05Navigazione">


    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="About"
            ContentTemplate="{DataTemplate local:AboutPage}"
            Route="AboutPage"/>
        <ShellContent
            Title="Insert"
            ContentTemplate="{DataTemplate local:InsertPage}"
            Route="InsertPage"/>
    </TabBar>
</Shell>
