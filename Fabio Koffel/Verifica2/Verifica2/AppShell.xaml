<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Verifica2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Verifica2"
    Shell.FlyoutBehavior="Disabled"
    Title="Verifica2">

    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />

        <ShellContent
            Title="CampiPage"
            ContentTemplate="{DataTemplate local:CampiPage}"
            Route="CampiPage" />
    </TabBar>
</Shell>
